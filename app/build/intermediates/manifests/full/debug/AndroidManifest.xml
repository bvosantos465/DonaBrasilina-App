<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luan.synthdecor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Dona Brasilina"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.byah_.donabrasilina.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.byah_.donabrasilina.DebugActivity" />
        <activity
            android:name="com.example.byah_.donabrasilina.HomeActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="com.example.byah_.donabrasilina.CartActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="com.example.byah_.donabrasilina.NotificationActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="com.example.byah_.donabrasilina.MyAccountActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="com.example.byah_.donabrasilina.OrderDetailActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name="com.example.byah_.donabrasilina.OrderActivity"
            android:parentActivityName="com.example.byah_.donabrasilina.HomeActivity"
            android:theme="@style/AppTheme.Toolbar" />
    </application>

</manifest>